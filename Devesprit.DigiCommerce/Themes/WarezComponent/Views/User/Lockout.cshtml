

@{
    ViewBag.Title = T("LockedOut");
    Html.AddHeadCustomParts("<meta name=\"robots\" content=\"noindex,nofollow\">");
}

<div class="container t-mar-35">
    <div class="row justify-content-center">
        <div class="col-12 col-lg-6">
            <div class="panel">
                <div class="panel-title pad-10">
                    <h5 class="no-mar">@T("LockedOut")</h5>
                </div>
                <div class="fs-14 pad-20 text-center">
                    <img src="~/Content/img/LockOut_180px.png" alt="Locked Out" />
                    <h4>@T("LockedOut")</h4>
                    <p>@T("LockedOutDesc")</p>
                    <a class="btn btn-outline-secondary btn-sm" href="@Url.Action("Index", "Home")">@T("ReturnHome")</a>
                </div>
            </div>
        </div>
    </div>
</div>

